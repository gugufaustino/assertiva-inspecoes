@model Seguradora

<div class="row">
    <fieldset class="col-md-12 form-group">
        <legend class="col-form-legend col-sm-12">
            <label>Relacionamento</label>
        </legend>
        <div class="form-group col-md-12">
            <div class="form-group col-md-4">
                @Html.FwLabelFor(model => model.EmailRemetenteSolicitacao)
                @Html.FwTextBoxFor(model => model.EmailRemetenteSolicitacao)
            </div>
            <div class="form-group col-md-3">
                @Html.FwLabelFor(model => model.IndAgendaRepostaPorEmail, htmlAttributes: new { @class = "control-label" })
                @Html.FwCheckBoxFor(model => model.IndAgendaRepostaPorEmail)
            </div>
            <div class="form-group col-md-3">
                @Html.FwLabelFor(model => model.IndLaudoRepostaPorEmail)
                @Html.FwCheckBoxFor(model => model.IndLaudoRepostaPorEmail)
            </div>
        </div>
    </fieldset>

    <fieldset class="col-md-12 form-group">
        <legend class="col-form-legend col-sm-12">
            <label>Integração</label>
        </legend>
        <div class="form-group col-md-12">
            <div class="form-group col-md-3">
                @Html.FwLabelFor(model => model.IndIntegracaoSolicitacaoPorEmail)
                @Html.FwCheckBoxFor(model => model.IndIntegracaoSolicitacaoPorEmail)
            </div>
        </div>
    </fieldset>
</div>
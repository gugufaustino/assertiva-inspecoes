@model  Seguradora
@using System.Linq
<div class="row">

    <fieldset class="col-md-6 form-group">
        <legend class="col-form-legend col-sm-12">
            <label>Cláusula de Quilometragem</label>
        </legend>
        <div class="form-group col-md-5">

            @Html.FwLabelFor(m => m.QtdQuilometroFranquia)
            @Html.FwCurrencyFor(m => m.QtdQuilometroFranquia)

        </div>

        <div class="form-group col-md-7">
            @Html.FwLabelFor(m => m.VlrQuilometroExcedente)
            @Html.FwCurrencyFor(m => m.VlrQuilometroExcedente)
        </div>
    </fieldset>


</div>


<div class="row">
    <fieldset class="form-group col-sm-6">
        <legend class="col-form-legend col-sm-12">Seguradora paga inspeções enviadas</legend>

        <div class="form-group col-md-6">
            @Html.FwLabelFor(model => model.ContabilInspecoesDiaInicio )
            @Html.FwTextBoxFor(model => model.ContabilInspecoesDiaInicio)
           
        </div>
        <div class="form-group col-md-6">
            @Html.FwLabelFor(model => model.ContabilInspecoesDiaFim )
            @Html.FwTextBoxFor(model => model.ContabilInspecoesDiaFim)
          
        </div>


    </fieldset>
    <div class="col-md-1">
    </div>
    <fieldset class="form-group col-sm-5">
        <legend class="col-form-legend col-sm-12">Dia lançamento sintético</legend>

        <div class="form-group col-md-6">
            @Html.FwLabelFor(model => model.ContabilInspetorDia )
            @Html.FwTextBoxFor(model => model.ContabilInspetorDia)
            
        </div>
        <div class="form-group col-md-6">
            @Html.FwLabelFor(model => model.ContabilEmpresaDia )
            @Html.FwTextBoxFor(model => model.ContabilEmpresaDia) 
        </div>

    </fieldset>
</div>








